{
  "manifest_version": 2,
  "name": "Sad Animal Facts New Tab",
  "description": "See a random 'sad animal fact' in new tab.",
  "version": "1.0.1",
  "background": {
    "scripts": ["page.js"],
    "persistent": false
  },
  "icons": {
    "128": "plus_icon.png"
  },
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "web_accessible_resources": ["fonts/Schoolbell-Regular.ttf"]
}
